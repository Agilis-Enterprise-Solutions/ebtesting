<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	     <!-- Paper Format for Soil Abrasion Report -->
		<record model="report.paperformat" id="report_soil_abrasion">
			<field name="name">Soil Abrasion</field>
			<field name="model">skit.soil.abarsion</field>
			<field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">300</field>
            <field name="page_width">210</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">30</field>
            <field name="margin_bottom">23</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">20</field>
            <field name="dpi">130</field>
			<field name="report_name">skit_project.report_soil_abrasion_document</field>
		</record>
         <!--  Soil Abrasion Report -->
         <report 
			model="skit.soil.abrasion" 
			id="action_report_soil_abrasions" 
			name="skit_project.report_soil_abrasion_document_advance" 
			file="skit_project.report_soil_abrasion_document_advance" 
			report_type="qweb-pdf" paperformat="skit_project.report_soil_abrasion"  string="Print Advance Copy"/>
         
		<report 
			model="skit.soil.abrasion" 
			id="action_report_soil_abrasion" 
			name="skit_project.report_soil_abrasion_document" 
			file="skit_project.report_soil_abrasion_document" 
			report_type="qweb-pdf" paperformat="skit_project.report_soil_abrasion"  string="Print Final Copy"/>
	</data>
</odoo>